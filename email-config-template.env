# Email Configuration for Application Form Notifications
# Add these variables to your .env file

# SMTP Server Configuration
SMTP_HOST=mcp-smtp-server
SMTP_PORT=587
SMTP_USER=your-email@example.com
SMTP_PASSWORD=your-password
SENDER_EMAIL=noreply@yourbreedingprogram.com

# Optional: Configure email notification behavior
# Set to 'false' to disable email notifications
ENABLE_EMAIL_NOTIFICATIONS=true
